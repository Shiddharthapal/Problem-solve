#include<bits/stdc++.h>
using namespace std;
typedef unsigned long long ll;
ll esp=1e+9;
int main(){
  int t;
  cin>>t;
  while(t--){
    int n;
    cin>>n;
    int a[n],x=0;
    for(int i=0;i<n;i++){
      cin>>a[i];
      if(a[i]==1) x=i;
    }
    if(x!=0) cout<<"NO"<<endl;
    else cout<<"YES"<<endl;
  }
}

#include <bits/stdc++.h>
using namespace std;
 
int main() 
{
    int t;
    cin>>t;
    while(t--){
    int n;
    cin>>n;
    string s;
    cin>>s;
    int count1=0,count2=0,count3=0;
    for(int i=0;i<n;i++){
      if(s[i]=='0')count1++;
    }
    
    if(count1>0) cout<<"YES"<<endl;
    else cout<<"NO"<<endl;
    }
}

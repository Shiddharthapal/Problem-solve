#include <bits/stdc++.h>
using namespace std;
 
int main() 
{
    string s;
    cin>>s;
    char x=s[0],y=s[1];
    int flag=0;
    for(int i=0;i<5;i++){
      string a;
      cin>>a;
      char c=a[0];
      char d=a[1];
      if(x==c || y==d){
        flag=1;
      }
    }
    if(flag==1){
      cout<<"YES"<<endl;
    }else{
      cout<<"NO"<<endl;
    }
}

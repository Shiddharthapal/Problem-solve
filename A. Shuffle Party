#include <iostream>
using namespace std;

int main() 
{
  int n;
  cin>>n;
  while(n--){
    long long sum=4;
    int number;
    cin>>number;
    if(number==1){
      cout<<1<<endl;
      continue;
    }
    while(number>=sum){
      sum=sum*2;
    }
    (number==sum)?cout<<sum:cout<<sum/2<<endl;
  }
  
  return 0;
}
